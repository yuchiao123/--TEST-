      "source": [
        "# 引入必要的庫\n",
        "import matplotlib.pyplot as plt\n",
        "from sklearn.datasets import load_iris\n",
        "import pandas as pd\n",
        "\n",
        "# 載入鳶尾花數據集\n",
        "iris = load_iris()\n",
        "\n",
        "# 將數據轉換為 DataFrame\n",
        "df = pd.DataFrame(data=iris.data, columns=iris.feature_names)\n",
        "df['species'] = iris.target\n",
        "\n",
        "# 將 species 轉換為文字標籤\n",
        "species_map = {0: 'Setosa', 1: 'Versicolor', 2: 'Virginica'}\n",
        "df['species'] = df['species'].map(species_map)\n",
        "\n",
        "# 繪製散點圖\n",
        "plt.figure(figsize=(10, 6))\n",
        "for species in df['species'].unique():\n",
        "    species_df = df[df['species'] == species]\n",
        "    plt.scatter(species_df['sepal length (cm)'], species_df['sepal width (cm)'], label=species)\n",
        "\n",
        "plt.xlabel('花萼長度 (cm)')\n",
        "plt.ylabel('花萼寬度 (cm)')\n",
        "plt.title('鳶尾花花萼長寬散點圖')\n",
        "plt.legend()\n",
        "plt.show()\n",
        "\n",
        "# 繪製箱形圖\n",
        "plt.figure(figsize=(10, 6))\n",
        "plt.subplot(1, 2, 1)\n",
        "plt.boxplot([df.loc[df['species'] == 'Setosa','sepal length (cm)'],\n",
        "             df.loc[df['species'] == 'Versicolor','sepal length (cm)'],\n",
        "             df.loc[df['species'] == 'Virginica','sepal length (cm)']],\n",
        "             labels=['Setosa', 'Versicolor', 'Virginica'])\n",
        "plt.title('花萼長度箱形圖')\n",
        "plt.ylabel('花萼長度 (cm)')\n",
        "\n",
        "plt.subplot(1, 2, 2)\n",
        "plt.boxplot([df.loc[df['species'] == 'Setosa','sepal width (cm)'],\n",
        "             df.loc[df['species'] == 'Versicolor','sepal width (cm)'],\n",
        "             df.loc[df['species'] == 'Virginica','sepal width (cm)']],\n",
        "             labels=['Setosa', 'Versicolor', 'Virginica'])\n",
        "plt.title('花萼寬度箱形圖')\n",
        "plt.ylabel('花萼寬度 (cm)')\n",
        "\n",
        "plt.tight_layout()\n",
        "plt.show()\n",
        "\n",
        "# 繪製直方圖\n",
        "plt.figure(figsize=(10, 6))\n",
        "plt.subplot(1, 2, 1)\n",
        "plt.hist(df['sepal length (cm)'], bins=10, alpha=0.7, color='blue', edgecolor='black')\n",
        "plt.title('花萼長度直方圖')\n",
        "plt.xlabel('花萼長度 (cm)')\n",
        "plt.ylabel('頻率')\n",
        "\n",
        "plt.subplot(1, 2, 2)\n",
        "plt.hist(df['sepal width (cm)'], bins=10, alpha=0.7, color='blue', edgecolor='black')\n",
        "plt.title('花萼寬度直方圖')\n",
        "plt.xlabel('花萼寬度 (cm)')\n",
        "plt.ylabel('頻率')\n",
        "\n",
        "plt.tight_layout()\n",
        "plt.show()"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "from sklearn.linear_model import LinearRegression\n",
        "from sklearn.datasets import load_iris\n",
        "\n",
        "# 載入鳶尾花資料集\n",
        "iris = load_iris()\n",
        "X = iris.data[:, :2]  # 花萼長度和寬度\n",
        "y = iris.target\n",
        "\n",
        "# 建立線性回歸模型\n",
        "model = LinearRegression()\n",
        "model.fit(X, y)\n",
        "\n",
        "# 繪製散點圖和預測線\n",
        "plt.scatter(X[:, 0], X[:, 1], c=y)\n",
        "plt.plot(X[:, 0], model.predict(X), color='red')\n",
        "\n",
        "# 設定圖片標題和軸標籤\n",
        "plt.title('Iris Dataset Linear Regression')\n",
        "plt.xlabel('Sepal Length')\n",
        "plt.ylabel('Sepal Width')\n",
        "\n",
        "# 顯示圖片\n",
        "plt.show()"
      ],
